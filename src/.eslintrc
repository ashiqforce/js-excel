{
    "parser": "@babel/eslint-parser",
    "env": {
        "es6": true,
        "browser":true,
        "node": true
    },
    "parserOptions": {
        "requireConfigFile": false
      },
    "rules": {
        "semi":"off",
        "arrow-parens":"off",
        "comma-dangle":"off",
        "require-jsdoc":"off",
        "eol-last":"off"
    },
    "extends":["eslint:recommended", "google"]
}